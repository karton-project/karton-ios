  pod 'GoogleMLKit/TextRecognition'
  pod 'GoogleMLKit/PoseDetection'


  target 'karton-iosTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'karton-iosUITests' do
    # Pods for testing
  end

end
